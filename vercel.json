{
  "version": 2,
  "builds": [
    {
      "src": "frontend/dist/frontend/**",
      "use": "@vercel/static"
    },
    {
      "src": "backend/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/dist/frontend/index.html"
    }
  ]
}



